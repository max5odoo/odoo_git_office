<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="student_update_view" model="ir.ui.view">
        <field name="name">student.update.view</field>
        <field name="model">student.student</field>
        <field name="inherit_id" ref="manthan.student_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//div[hasclass('fruits')]//span[2]" position="replace">
                <span>Banana</span>
            </xpath>

            <xpath expr="//div[hasclass('vegetable')]//span[2]" position="replace">
                <span>Lady Finger</span>
            </xpath>


        </field>
    </record>
</odoo>